{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
   "parserOptions": {
       "parser": "babel-eslint"
   },
  "extends": [
      "airbnb-base",
      "plugin:vue/recommended"
  ],
  "rules": {
      "no-alert": "off",
      "no-console": "off",
      "linebreak-style": "off"
  }
}